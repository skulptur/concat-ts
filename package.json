{
  "name": "contac-ts",
  "private": true,
  "version": "1.0.0",
  "description": "Simple code to concats exported typescript files",
  "keywords": [
    "typescript",
    "cli",
    "starter"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "pkg": "^5.8.1",
    "rimraf": "^2.6.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "commander": "^10.0.0"
  }
}
